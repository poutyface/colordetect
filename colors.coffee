getColorName = (intR, intG, intB) ->
  diff = Number.MAX_VALUE;
  tmp = ""

  for i, v of colorNames
    codeR = colorNames[i][0]
    codeG = colorNames[i][1]
    codeB = colorNames[i][2]

    def = (Math.pow(intR-codeR, 2) + Math.pow(intG-codeG, 2) + Math.pow(intB-codeB, 2))

    if(def < diff)
      diff = def
      tmp = i

  return (new Array(tmp, colorNames[tmp]))


detect_color = (ctx, x, y) ->
  pixel = ctx.getImageData(x,y,1,1).data
  [r, g, b] = [pixel[0], pixel[1], pixel[2]]
#  $("#color_area").css(rgb(r, g, b))

  color_names = getColorName(r, g, b)
  color_name = color_names[0]
  color_name

insert_color_name = (name) ->
  $("#color_name").html("<p>Color Name :  #{name}</p>")


